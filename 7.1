def is_binary_string(s):
    # Create a set of unique characters in the string
    unique_chars = set(s)
    
    # If the set only contains '0' and '1', and the length is 2, it's a binary string
    if unique_chars == {'0', '1'} or unique_chars == {'0'} or unique_chars == {'1'}:
        return "Yes"
    else:
        return "No"


str1 = input()

print(is_binary_string(str1))
